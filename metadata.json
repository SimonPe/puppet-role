{
  "name": "vstone-role",
  "version": "0.1.0",
  "author": "Jan Vansteenkiste",
  "summary": "Map roles in different namespaces",
  "source": "https://github.com/vStone/puppet-role",
  "license": "MIT",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": "> 3.0.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    { "operatingsystem": "Debian", "operatingsystemrelease": ["8"] },
    { "operatingsystem": "RedHat", "operatingsystemrelease": [ "7.0" ] }
  ]
}
